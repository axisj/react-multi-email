{
  "extends": "../tsconfig.build.json",
  "compilerOptions": {
    "outDir": "../dist/commonjs",
    "module": "commonjs",
    "target": "es5",
    "downlevelIteration": true,
    "allowJs": false,
    "declaration": true,
    "sourceMap": false,
    "noEmit": false,
    "jsx": "react",
    "jsxImportSource": ""
  },
  "include": [
    "../react-multi-email/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/tree.ts"
  ]
}
